{
    "docs": {
        "branches.regex.filters": [
            "origin/master"
        ],
        "tags.semver.filters": [
            ">12.0.0,<=12.1.0"
        ],
        "tags.semver.relaxed": false,
        "scripts": [
            "sphinx-build -b html . ${build_path}/${name}"
        ],
        "cwd": "${source_path}/docs",
        "requirements": "${source_path}/docs/requirements.txt"
    }
}