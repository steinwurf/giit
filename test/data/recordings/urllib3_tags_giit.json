{
  "allow_failure": false,
  "branches": {
    "regex": {
      "filters": [
        "origin/master"
      ]
    },
    "source_branch": false
  },
  "cwd": "${source_path}/docs",
  "pip_packages": null,
  "python_path": "${source_path}/src",
  "requirements": "${source_path}/docs/requirements.txt",
  "scripts": [
    "sphinx-build -b html . ${build_path}/${name}"
  ],
  "tags": {
    "regex": {
      "filters": []
    },
    "semver": {
      "filters": [
        ">=1.20"
      ],
      "relaxed": true
    }
  },
  "variables": {},
  "workingtree": false
}